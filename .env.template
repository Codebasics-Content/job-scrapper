# BrightData API Configuration
# Copy this file to .env and fill in your actual values

BRIGHTDATA_API_TOKEN=your_brightdata_api_token_here
BRIGHTDATA_BASE_URL=https://api.brightdata.com
BRIGHTDATA_LINKEDIN_COLLECTOR_ID=your_linkedin_collector_id_here
BRIGHTDATA_INDEED_COLLECTOR_ID=your_indeed_collector_id_here

# Rate limiting (requests per second)
BRIGHTDATA_RATE_LIMIT_QPS=1.0
BRIGHTDATA_TIMEOUT_SECONDS=30